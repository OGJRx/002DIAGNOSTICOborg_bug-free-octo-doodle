{
  "version": 2,
  "builds": [
    {
      "src": "api/!(*.test).ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["flows/**"]
      }
    }
  ],
  "routes": [
    { "src": "/api/diagnose", "dest": "/api/diagnose.ts" },
    { "src": "/api/(.*)", "dest": "/api/index.ts" }
  ]
}
